	local un=Instance.new("TextLabel")un.Size=UDim2.new(1,-4,0,13)un.Position=UDim2.new(0,2,0,18)un.BackgroundTransparency=1 un.Text="@"..pl.Name un.TextColor3=Color3.fromRGB(180,180,180)un.TextSize=7 un.Font=Enum.Font.Gotham un.Parent=bg
	local hpB=Instance.new("Frame")hpB.Name="HPBar"hpB.Size=UDim2.new(1,-4,0,4)hpB.Position=UDim2.new(0,2,1,-7)hpB.BackgroundColor3=Color3.fromRGB(35,6,6)hpB.BorderSizePixel=0 hpB.Parent=bg cr(hpB,2)
	local fi=Instance.new("Frame")fi.Name="Fill"fi.Size=UDim2.new(1,0,1,0)fi.BackgroundColor3=Color3.fromRGB(80,220,80)fi.BorderSizePixel=0 fi.Parent=hpB cr(fi,2)tags[ch]=bb
end
local function utT(ch)
	local bb=tags[ch]if not bb or not bb.Parent then return end
	local hm=ch:FindFirstChildOfClass("Humanoid")if not hm then return end
	local pct=math.clamp(hm.Health/math.max(hm.MaxHealth,1),0,1)
	local bg=bb:FindFirstChildOfClass("Frame")if not bg then return end
	local hp=bg:FindFirstChild("HPBar")if not hp then return end
	local fi=hp:FindFirstChild("Fill")if not fi then return end
	fi.Size=UDim2.new(pct,0,1,0)fi.BackgroundColor3=Color3.fromRGB(math.floor((1-pct)*220),math.floor(pct*220),25)
end
local function stFly()
	fOn=false if fC then fC:Disconnect()fC=nil end
	if fBV and fBV.Parent then fBV:Destroy()end fBV=nil
	if fBG and fBG.Parent then fBG:Destroy()end fBG=nil
	local mc=LP.Character if mc then local h=mc:FindFirstChildOfClass("Humanoid")if h then h.PlatformStand=false end end
end
local function goFly()
	fOn=true local mc=LP.Character if not mc then return end
	local rt=mc:FindFirstChild("HumanoidRootPart")if not rt then return end
	local hm=mc:FindFirstChildOfClass("Humanoid")if not hm then return end
	hm.PlatformStand=true
	if fBV and fBV.Parent then fBV:Destroy()end if fBG and fBG.Parent then fBG:Destroy()end
	local bv=Instance.new("BodyVelocity")bv.Velocity=Vector3.zero bv.MaxForce=Vector3.new(1e5,1e5,1e5)bv.P=1e4 bv.Parent=rt fBV=bv
	local bg=Instance.new("BodyGyro")bg.MaxTorque=Vector3.new(1e5,0,1e5)bg.P=1e4 bg.D=500 bg.CFrame=rt.CFrame bg.Parent=rt fBG=bg
	local cam=workspace.CurrentCamera
	fC=RS.Heartbeat:Connect(function()
		if not fOn then return end
		local mc2=LP.Character if not mc2 then return end
		if not fBV or not fBV.Parent then return end
		local h2=mc2:FindFirstChildOfClass("Humanoid")if not h2 then return end
		local r2=mc2:FindFirstChild("HumanoidRootPart")if not r2 then return end
		local cf=cam.CFrame
		local mv=h2.MoveDirection local dir=Vector3.zero
		if mv.Magnitude>0 then
			local lm=cf:VectorToObjectSpace(mv)
			dir=(cf.LookVector*-lm.Z+cf.RightVector*lm.X)
			if dir.Magnitude>0 then dir=dir.Unit end
		end
		fBV.Velocity=dir*FS
		-- body hanya rotate sumbu Y, tidak ikut pitch kamera
		if fBG and fBG.Parent then
			local yaw=math.atan2(-cf.LookVector.X,-cf.LookVector.Z)
			fBG.CFrame=CFrame.new(r2.Position)*CFrame.Angles(0,yaw,0)
		end
	end)
end
RS.Stepped:Connect(function()
	local mc=LP.Character if not mc then return end
	if hbOn or nOn then
		for _,p in ipairs(mc:GetDescendants())do if p:IsA("BasePart")then p.CanCollide=false end end
	end
end)
RS.Heartbeat:Connect(function()
	for _,pl in ipairs(Pl:GetPlayers())do
		if pl==LP then continue end
		local ch=pl.Character if not ch then continue end
		local rt=ch:FindFirstChild("HumanoidRootPart")if not rt then continue end
		if hbOn then rt.Size=Vector3.new(HS,HS,HS)mkHB(ch,rt)else if hbDummy[ch]or hbSB[ch]then rmHB(ch)end end
		if eOn then mkEHL(ch)mkT(ch,pl)utT(ch)else if espHLs[ch]then rmEHL(ch)end if tags[ch]then rmT(ch)end end
	end
	if sOn and saTgt~=""then
		local tp=Pl:FindFirstChild(saTgt)
		if tp and tp.Character then
			local th=tp.Character:FindFirstChild("HumanoidRootPart")
			if th then local mc=LP.Character if mc then local rt=mc:FindFirstChild("HumanoidRootPart")if rt and(th.Position-rt.Position).Magnitude<200 then workspace.CurrentCamera.CFrame=CFrame.new(workspace.CurrentCamera.CFrame.Position,th.Position)end end end
		end
	end
	for ch in pairs(hbDummy)do if not ch.Parent then rmHB(ch)end end
	for ch in pairs(espHLs)do if not ch.Parent then rmEHL(ch)end end
	for ch in pairs(tags)do if not ch.Parent then rmT(ch)end end
end)
