local function showAC(query)
	for _,c in ipairs(saAC:GetChildren())do c:Destroy()end
	if query==""then saAC.Size=UDim2.new(1,0,0,0)return end
	local q=query:lower()
	local matches={}
	for _,pl in ipairs(Pl:GetPlayers())do if pl~=LP and pl.Name:lower():sub(1,#q)==q then table.insert(matches,pl.Name)end end
	local yy=0
	for _,name in ipairs(matches)do
		local b=Instance.new("TextButton")b.Size=UDim2.new(1,0,0,26)b.Position=UDim2.new(0,0,0,yy)b.BackgroundColor3=Color3.fromRGB(20,12,45)b.BorderSizePixel=0
		b.Text=" "..name b.TextColor3=Color3.fromRGB(200,180,255)b.TextSize=10 b.Font=Enum.Font.Gotham b.TextXAlignment=Enum.TextXAlignment.Left b.ZIndex=12 b.Parent=saAC cr(b,4)
		b.MouseButton1Click:Connect(function()saTgt=name saIn.Text=name saL.Text="Target: "..name for _,c in ipairs(saAC:GetChildren())do c:Destroy()end saAC.Size=UDim2.new(1,0,0,0)end)
		yy=yy+28
	end
	saAC.Size=UDim2.new(1,0,0,yy)
end
saIn:GetPropertyChangedSignal("Text"):Connect(function()showAC(saIn.Text)end)
saIn.FocusLost:Connect(function()
	if saIn.Text~=""then local q=saIn.Text:lower()for _,pl in ipairs(Pl:GetPlayers())do if pl~=LP and pl.Name:lower():sub(1,#q)==q then saTgt=pl.Name saIn.Text=pl.Name saL.Text="Target: "..pl.Name break end end end
	for _,c in ipairs(saAC:GetChildren())do c:Destroy()end saAC.Size=UDim2.new(1,0,0,0)
end)
local function rmHB(ch)
	if hbSB[ch]and hbSB[ch].Parent then hbSB[ch]:Destroy()end hbSB[ch]=nil
	if hbDummy[ch]and hbDummy[ch].Parent then hbDummy[ch]:Destroy()end hbDummy[ch]=nil
end
local function mkHB(ch,rt)
	if not hbDummy[ch]or not hbDummy[ch].Parent then
		local d=Instance.new("Part")d.Name="HBX_D"d.Anchored=true d.CanCollide=false d.CanTouch=false d.CanQuery=false d.Massless=true d.CastShadow=false d.Transparency=1 d.Parent=workspace hbDummy[ch]=d
	end
	hbDummy[ch].Size=Vector3.new(HS,HS,HS)hbDummy[ch].CFrame=rt.CFrame
	if not hbSB[ch]or not hbSB[ch].Parent then
		local sb=Instance.new("SelectionBox")sb.Color3=Color3.fromRGB(255,0,0)sb.LineThickness=0.04 sb.SurfaceTransparency=0.65 sb.SurfaceColor3=Color3.fromRGB(255,60,60)sb.Adornee=hbDummy[ch]sb.Parent=workspace.CurrentCamera hbSB[ch]=sb
	end
end
